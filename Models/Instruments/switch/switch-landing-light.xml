<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>switch-l.ac</path>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>switch</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
				<property>controls/lighting/landing-light</property>
      </binding>
    </action>
  </animation>
  
    <animation>
        <type>rotate</type>
        <object-name>switch</object-name>
				<property>controls/lighting/landing-light</property>
        <factor>-60</factor>
        <!--center>
            <x-m>-0.041</x-m>
            <y-m>-0.197</y-m>
            <z-m>0.417</z-m>
        </center-->
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
  
</PropertyList>
