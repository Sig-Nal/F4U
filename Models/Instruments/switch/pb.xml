<?xml version="1.0"?>
<PropertyList>

 <path>pb.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <emission>
    <!--factor-prop>/controls/lighting/instruments-norm</factor-prop-->
    <red>0.35</red>
    <green>0.35</green>
    <blue>0.35</blue>
  </emission>
 </animation>

<effect>
 <inherits-from>Effects/model-transparent</inherits-from>
  <type>material</type>
    <object-name>Face</object-name>
          <condition>
             <property>sim/rendering/rembrandt/enabled</property>
         </condition>
  </effect> 

<animation>
  <type>translate</type>
  <object-name>Face</object-name>
  <property>controls/armament/master-arm</property>  
  <factor>0.001</factor>
  <!--axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis-->
</animation>


